{
  "name": "git-cleaner",
  "version": "1.0.7",
  "description": "CLI utility for bulk cleanup of branches and tags",
  "main": "dist/cli.js",
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json",
    "clean": "trash dist",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md && npm run build",
    "commit": "npx git-cz",
    "lint": "eslint . --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "gitc": "dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobinCK/git-cleaner.git"
  },
  "keywords": [
    "git",
    "cli",
    "branch",
    "tag"
  ],
  "author": "Igor Ognichenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RobinCK/git-cleaner/issues"
  },
  "homepage": "https://github.com/RobinCK/git-cleaner#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "cli-progress": "^3.8.2",
    "commander": "^6.0.0",
    "prompts": "^2.3.2",
    "simple-git": "^3.15.0",
    "yaml-lint": "^1.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@types/chalk": "^2.2.0",
    "@types/cli-progress": "^3.8.0",
    "@types/commander": "^2.12.2",
    "@types/node": "^14.6.0",
    "@types/prompts": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "conventional-changelog-cli": "^2.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.7.0",
    "husky": "^4.2.5",
    "jsonlint": "^1.6.3",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "trash-cli": "^3.1.0",
    "typescript": "^3.9.7"
  }
}
